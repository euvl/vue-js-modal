{
  "name": "vue-js-modal",
  "description": "Modal Component for Vue.js",
  "version": "1.3.27",
  "author": "euvl <yev.vlasenko@gmail.com>",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
